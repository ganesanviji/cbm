<!-- Body Starts -->
<body class="layoutbody" cz-shortcut-listen="true">
    <div id="title"></div>
    
    <div style="width: auto; height: 83px;">
        <div id="logo" class="logo">
            <div style="height: 110px; margin-left: auto;margin-right: auto;width: 85%;">
                <p style="float: left; width: 11%; margin-left: 0%">
                    <img src="../images/logo.png" width="116" height="84">
                </p>
                <p style="float: left; margin-top: 5%; width: 50%; margin-left: 10%; color: #3050A3; height: 25%;">
                    <span id="font">CBM Forms </span>
                </p>
            </div>
        </div>
    </div>
    <div class="clear"></div>
    <div id = "main1">
    

    <h2 class="pageheader">CBM Login</h2>    
    
    <%= form_for(resource, as: resource_name, url: session_path(resource_name), :novalidate =>"novalidate") do |f| %>
    
        <fieldset id="field">
            <div style="min-height: 150px;">
                <h1></h1>
                <table style="border: none ! important">
                    <tbody>
                        <tr>
                            <td style="border: none ! important">
                                <span>
                                    <label for="UserName">Email</label></span>
                            </td>
                            <td style="border: none ! important">
                            	<%= f.email_field :email, autofocus: true %>
                                <span class="field-validation-valid" data-valmsg-for="UserName" data-valmsg-replace="true"></span>
                            </td>
                        </tr>
                        <tr>
                            <td style="border: none ! important">
                                <span>Password </span>
                            </td>
                            <td style="border: none ! important">
                            	<%= f.password_field :password %>
                                
                                <span class="field-validation-valid" data-valmsg-for="Password" data-valmsg-replace="true"></span>
                            </td>
                        </tr>
                        <tr>
                            <td style="border: none ! important"></td>
                            <td style="border: none ! important">
                            	<%= f.submit "Sign in", :id=>"btnstafflogin", :title=>"Login", :class=>"operationSavebutton"  %>
                                
                                
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </fieldset>
    
    <% end %>
   <%= render "devise/shared/links" %> 
</div>
    <!-- Body Ends -->

