    
    <html class="k-ff k-ff20"><head>
<meta http-equiv="content-type" content="text/html; charset=windows-1252">
      <title>Home Page</title>
      <link href="/stylesheets/Site.css" rel="stylesheet" type="text/css">
      <link href="/stylesheets/FormStyles.css" rel="stylesheet">
      <link href="/stylesheets/kendo.css" rel="stylesheet">
      <link href="/stylesheets/kendo_002.css" rel="stylesheet">
      <link href="/stylesheets/ControlStyles.css" rel="stylesheet">
      <link href="/stylesheets/GridStyles.css" rel="stylesheet">
      <link href="/stylesheets/LayoutStyles.css" rel="stylesheet">
      <link href="/stylesheets/ImageStyles.css" rel=" stylesheet">
      <link rel="stylesheet" type="text/css" href="/stylesheets/custom.css">
      <script src="../javascripts/jquery-1.js" type="text/javascript"></script>
      <script src="/javascripts/jquery.js" type="text/javascript"></script>    
      <script src="/javascripts/Moment.js" type="text/javascript"></script>
      <script src="/javascripts/kendo.js" type="text/javascript"></script>
      <script type="text/javascript">
        $(document).ready(function () {
           ShowTime();
        });
        function ShowTime() {
           var now = moment().format("ddd, MMM Do, YYYY, h:mm:ss A");
           var splitedDates = now.split(',');
           document.getElementById("lblday").innerHTML = splitedDates[0];
           document.getElementById("lblmonth").innerHTML = splitedDates[1];
           document.getElementById("lblyear").innerHTML = splitedDates[2];
           document.getElementById("lblTym").innerHTML = splitedDates[3];
           window.setTimeout("ShowTime()", 1000);
           //var dt = new Date();
           //document.getElementById("lblTym").innerHTML = dt.toLocaleTimeString();
           //window.setTimeout("ShowTime()", 1000); // Here 1000(milliseconds) means one 1 Sec  
        }
      </script>  
      <script>
        jQuery(function(){jQuery("#panelbar").kendoPanelBar({"expandMode":"multiple"});});
      </script>
  </head>
<!-- Body Content Starts -->
        <div id="main" class="main">
          <div style="height:1500px;">
            <link href="../stylesheets/ControlStyles.css" rel="stylesheet" type="text/css">
            <link href="/stylesheets/ControlStyles.css" rel="stylesheet">
            <link href="/stylesheets/GridStyles.css" rel="stylesheet">
            <link href="/stylesheets/LayoutStyles.css" rel="stylesheet">
            <link href="/stylesheets/ImageStyles.css" rel=" stylesheet">
  
            <p style="background-color:#283385;color:white;padding:0px;"> 
              <img src="/images/pin_24x24.png" style="vertical-align:middle;padding:0.5%;" height="24" width="24">
              <span class="topheaders">Edit Model</span>
            </p>

            <%= form_for([@reference,@model]) do |f| %>
              <div id="divMain">
               <!-- <input data-val="true" data-val-number="The field CourseID must be a number." data-val-required="The CourseID field is required." id="CourseID" name="CourseID" value="" type="hidden">
                <input id="ExpiryDate" name="ExpiryDate" value="" type="hidden">
                <input id="hidisexpiry" name="hidisexpiry" value="No" type="hidden">
                <div id="divHeader" style="width: 100%;">-->
                <fieldset>
          
                  <legend class="legendStyle">Edit Detail</legend>

                  <div class="clearfix" style="height: 20px;"></div>

                  <div>
                    <div class="divalignleft">

                      <div class="cdetaillabel">
                        <label for="CourseCategoryID">REF No</label>
                      </div>
                      <div class="cdetailright" style="display: inline-flex;">
                        
                          <%= f.select :reference_id, options_for_select(Reference.all.collect{ |u| [u.ref_no, u.id] }), :id=>"CourseCategoryCombo" %>
                        </span>
                        
                        <span class="field-validation-valid" data-valmsg-for="CourseCategoryID" data-valmsg-replace="true"></span>
                            
                      </div>
                      <div style="height: 20px;" class="clearfix"></div>

                        <div class="cdetaillabel">
                          <label for="CourseName">Model No</label>
                        </div>
                        <div class="cdetailright">
                          <%= f.text_field :model_no, :class=> "text-box single-line", :id => "CourseName" %>
                          
                          <span class="field-validation-valid" data-valmsg-for="CourseName" data-valmsg-replace="true"></span>
                          <!--<input id="CourseName" name="CourseName" value="" type="hidden">-->
                        </div>

                        <div class="clearfix" style="height: 20px;"></div>

                        <div class="cdetaillabel">
                          <label for="CourseName">Description</label>
                        </div>
                        <div class="cdetailright">
                          <%= f.text_area :description, :class => "text-box single-line", :id => "CourseName", :style => "width:226px;border:1px solid #CCCCCC;" %>
                          
                          <span class="field-validation-valid" data-valmsg-for="CourseName" data-valmsg-replace="true"></span>
                         <!-- <input id="CourseName" name="CourseName" value="" type="hidden">-->
                        </div>

                        <div class="clearfix" style="height: 20px;"></div>

                        <div class="cdetaillabel">
                          <label for="CourseName">Purchase Price</label>
                        </div>
                        <div class="cdetailright">
                          <%= f.text_field :pur_price, :class=>"text-box single-line", :id => "CourseName" %>

                          <span class="field-validation-valid" data-valmsg-for="CourseName" data-valmsg-replace="true"></span>
                          <!--<input id="CourseName" name="CourseName" value="" type="hidden">-->
                        </div>

                        <div style="height: 20px;" class="clearfix"></div>

                      </div>

                    <div class="divalignright">

                      <div class="cdetaillabel">
                        <label for="CourseName">CBM</label>
                      </div>
                      
                      <div class="cdetailright">
                        <%= f.text_field :cbm, :class=>"text-box single-line", :id => "CourseName" %>
                          
                          <span class="field-validation-valid" data-valmsg-for="CourseName" data-valmsg-replace="true"></span>
                          <!--<input id="CourseName" name="CourseName" value="" type="hidden">-->
                      </div>

                      <div class="clearfix" style="height: 20px;"></div>

                      <div class="cdetaillabel">
                        <label for="CourseName">Weight</label>
                      </div>
                      
                      <div class="cdetailright">
                        <%= f.text_field :weight, :class=>"text-box single-line", :id => "CourseName" %>
                          <span class="field-validation-valid" data-valmsg-for="CourseName" data-valmsg-replace="true"></span>
                          <!--<input id="CourseName" name="CourseName" value="" type="hidden">-->
                      </div>

                      <div class="clearfix" style="height: 20px;"></div>

                      <div class="cdetaillabel">
                        <label for="CourseName">Selling Price</label>
                      </div>
                      
                      <div class="cdetailright">
                        <%= f.text_field :selling_price, :class=>"text-box single-line", :id => "CourseName" %>
                          <span class="field-validation-valid" data-valmsg-for="CourseName" data-valmsg-replace="true"></span>
                          <!--<input id="CourseName" name="CourseName" value="" type="hidden">-->
                      </div>

                      <div class="clearfix" style="height: 20px;"></div>

                      <div class="cdetaillabel">
                        <label for="CourseName">Upload Image </label>
                      </div>
                      <div class="cdetaillabel" id="imgcourseLogo">
                        <%= f.file_field :avatar, :style=>"width:20px;height:30px;" %>
                        
                      </div>
                        
                      

                      <div class="clearfix" style="height: 20px;"></div>

                    </div>
                  </div>

                </fieldset>
                <p style="text-align: center;">
                  
                  <%= f.submit "Finish", :id=>"btnsubmit", :class=>"operationSavebutton" %>
                </p>
              </div>
            
          </div><% end %>
       
        </div>
      </div>
        <!-- Body Content Ends -->
      </div>

    </div>
    
    <div class="clearfix"></div>
    
  
</body></html>
    
















